
@{
    ViewBag.Title = "Index";
}
@{
    Layout = null;
}
<div class="main-wrapper">
    <div class="account-page">
        <div class="container">
            <h3 class="account-title text-white"> Student Login </h3>
            
            <div class="account-box">
                <div class="account-wrapper">
                    <div class="account-logo">
                        <a href="Index.cshtml" class="logo">
                            <img src="~/assets/img/logo.png" width="40" height="60" alt="">
                        </a>
                    </div>

                    <form action="~/studentLogin/Login" method="post">



                        <label name="info"> @ViewBag.error </label>
                        <div class="form-group custom-mt-form-group">


                            <input type="text" class="form-control" name="userName" id="userName" />

                            <label class="control-label">Username</label><i class="bar"></i>
                        </div>
                        <div class="form-group custom-mt-form-group">
                            <label>Password</label>
                            <input type="password" class="form-control" name="password" id="password" />
                        </div>
                        <div class="form-group text-center custom-mt-form-group">
                            <button type="submit" name="submit" class="btn btn-primary btn-block account-btn" value="Login">Login</button>
                            <a href="~/stafflogin/Index" style="color:blue"><u>Staff Login </u></a>
                        </div>
                        
                        @*  }*@
                    </form>

                   
                </div>
            </div>
        </div>
    </div>
</div>

